{
  "_comment": "Example App - Showcases all override system features",
  "_comment_features": "This example demonstrates: custom title, logo override, menu item override, position control, custom menu items, theme customization, and submenu override",

  "page": {
    "title": "Example App Dashboard"
  },

  "sidebar": {
    "logoUrl": "https://mypancho.com/account/assets/img/logo.png",

    "navItems": [
      {
        "_position": "before",
        "_comment": "This item appears at the very beginning",
        "type": "link",
        "href": "#/priority-action.html",
        "label": "‚≠ê Priority Action",
        "icon": "star",
        "iframeUrl": "https://mypancho.com/account/homepage.php"
      },
      {
        "_override": "label",
        "_comment": "This overrides the default 'Homepage' menu item by matching its label",
        "type": "link",
        "href": "#/index.html",
        "label": "üè† My Custom Home",
        "icon": "home",
        "iframeUrl": "https://mypancho.com/account/homepage.php"
      },
      {
        "_override": "path",
        "_comment": "This overrides the 'Support' submenu",
        "type": "submenu",
        "path": "support",
        "label": "üéß Premium Support",
        "icon": "headset",
        "children": [
          {
            "_position": "before",
            "type": "link",
            "href": "#/vip-support.html",
            "label": "VIP Support Line",
            "icon": "phone",
            "iframeUrl": "https://mypancho.com/account/supporttickets.php"
          },
          {
            "_override": "label",
            "_comment": "Override specific item in submenu",
            "type": "link",
            "href": "#/open-support-ticket.html",
            "label": "Create Premium Ticket",
            "icon": "ticket-plus",
            "iframeUrl": "https://mypancho.com/account/supporttickets.php"
          }
        ]
      },
      {
        "_position": "after",
        "_comment": "This custom submenu appears at the end",
        "type": "submenu",
        "path": "custom-features",
        "label": "Custom Features",
        "icon": "sparkles",
        "children": [
          {
            "type": "link",
            "href": "#/feature-1.html",
            "label": "Feature One",
            "icon": "zap",
            "iframeUrl": "https://mypancho.com/account/homepage.php"
          },
          {
            "type": "link",
            "href": "#/feature-2.html",
            "label": "Feature Two",
            "icon": "zap",
            "iframeUrl": "https://mypancho.com/account/homepage.php"
          }
        ]
      },
      {
        "type": "header",
        "label": "App-Specific Section"
      },
      {
        "type": "link",
        "href": "#/special-tool.html",
        "label": "Special Tool",
        "icon": "wrench",
        "iframeUrl": "https://mypancho.com/account/homepage.php"
      }
    ]
  },

  "theme": {
    "_comment": "Only override the colors you want to change",
    "primary": "hsl(280, 70%, 60%)",
    "headerBg": "#80a3ff"
  },

  "messages": {
    "welcome": {
      "title": "Welcome to Example App",
